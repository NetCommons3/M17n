{
    "name":        "netcommons/m17n",
    "description": "M17n Plugin for CakePHP",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "m17n"],
    "homepage":    "http://www.netcommons.org/",
    "license":     "NetCommons License",
    "authors": [
        {
            "name": "NetCommons Community",
            "homepage": "http://www.netcommons.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations":            "~2.2",
        "netcommons/net-commons":       "@dev"
    },
    "config": {
        "vendor-dir": "vendors",
        "process-timeout": 3000
    }
}
